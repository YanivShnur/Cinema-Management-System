		<Text fill="ORANGE" layoutX="10.0" layoutY="33.0"
			strokeType="OUTSIDE" strokeWidth="0.0" text="Cancel Purchase"
			textAlignment="CENTER" wrappingWidth="640.0">
			<font>
				<Font name="Aharoni Bold" size="30.0" />
			</font>
		</Text>
		<Button fx:id="backButton" layoutX="562.0" layoutY="10.0"
			mnemonicParsing="false" onAction="#handheldsBackButton"
			prefHeight="25.0" prefWidth="61.0"
			style="-fx-background-color: ORANGE; -fx-background-radius: 10;"
			text="back" textAlignment="CENTER">
			<font>
				<Font name="System Bold" size="12.0" />
			</font>
			<cursor>
				<Cursor fx:constant="HAND" />
			</cursor>
		</Button>
		<SplitPane dividerPositions="0.4" layoutY="80.0"
