							layoutX="111.0" layoutY="368.0" mnemonicParsing="false"
							onAction="#handheldsCancelTicket"
							style="-fx-background-color: ORANGE;" text="Cancel Ticket">
							<font>
								<Font name="System Bold" size="24.0" />
							</font>
							<cursor>
								<Cursor fx:constant="HAND" />
							</cursor>
						</Button>
					</children>
				</AnchorPane>
			</items>
		</SplitPane>
	</children>
</AnchorPane>
