							<font>
								<Font name="System Bold" size="24.0" />
							</font>
						</Text>
						<Text fill="ORANGE" layoutX="50.0" layoutY="90.0"
							strokeType="OUTSIDE" strokeWidth="0.0" text="Movie name:"
							underline="true" AnchorPane.bottomAnchor="339.486328125"
							AnchorPane.leftAnchor="49.48828125"
							AnchorPane.rightAnchor="250.51171875"
							AnchorPane.topAnchor="74.892578125">
							<font>
								<Font size="14.0" />
							</font>
						</Text>
						<Text fill="ORANGE" layoutX="50.0" layoutY="150.0"
							strokeType="OUTSIDE" strokeWidth="0.0" text="Location:"
							underline="true">
							<font>
								<Font size="14.0" />
							</font>
						</Text>
						<Text fill="ORANGE" layoutX="50.0" layoutY="210.0"
							strokeType="OUTSIDE" strokeWidth="0.0" text="Theater"
							underline="true">
							<font>
								<Font size="14.0" />
							</font>
						</Text>
						<Text fill="ORANGE" layoutX="50.0" layoutY="270.0"
							strokeType="OUTSIDE" strokeWidth="0.0" text="Screening Time:"
							underline="true">
							<font>
								<Font size="14.0" />
							</font>
						</Text>
						<Text fill="ORANGE" layoutX="50.0" layoutY="330.0"
							strokeType="OUTSIDE" strokeWidth="0.0" text="Seats:"
							underline="true">
							<font>
								<Font size="14.0" />
							</font>
						</Text>
						<TextField id="movieName" fx:id="movieName"
							layoutX="180.0" layoutY="70.0" text="name" />
						<TextField id="locationName" fx:id="locationName"
							layoutX="180.0" layoutY="130.0" text="place" />
						<TextField id="theaterName" fx:id="theaterName"
							layoutX="180.0" layoutY="190.0" text="theater" />
						<TextField fx:id="screeningTime" layoutX="180.0"
							layoutY="250.0" text="time" />
						<TextField id="seats" fx:id="seats" layoutX="180.0"
							layoutY="310.0" text="seats" />
